<fo:root xmlns:fo="http://www.w3.org/1999/XSL/Format">
  <fo:layout-master-set>
    <fo:simple-page-master
      master-name="first"
      margin-right="1in"
      margin-left="1in"
      margin-bottom="1in"
      margin-top="1in"
      page-width="8in"
      page-height="11in">
      <fo:region-body></fo:region-body>
    </fo:simple-page-master>
  </fo:layout-master-set>
  <fo:page-sequence
    master-reference="first">
    <fo:flow flow-name="xsl-region-body" font-size="20pt">
      <fo:block>Test background-color property with the 16 named colors.</fo:block>
      <fo:block background-color="aqua" >aqua</fo:block>
      <fo:block background-color="black" >black</fo:block>
      <fo:block background-color="blue" >blue</fo:block>
      <fo:block background-color="fuchsia" >fuchsia</fo:block>
      <fo:block background-color="gray" >gray</fo:block>
      <fo:block background-color="green" >green</fo:block>
      <fo:block background-color="lime" >lime</fo:block>
      <fo:block background-color="maroon" >maroon</fo:block>
      <fo:block background-color="navy" >navy</fo:block>
      <fo:block background-color="olive" >olive</fo:block>
      <fo:block background-color="purple" >purple</fo:block>
      <fo:block background-color="red" >red</fo:block>
      <fo:block background-color="silver" >silver</fo:block>
      <fo:block background-color="teal" >teal</fo:block>
      <fo:block background-color="white" >white</fo:block>
      <fo:block background-color="yellow" >yellow</fo:block>
    </fo:flow>
  </fo:page-sequence>
</fo:root>
